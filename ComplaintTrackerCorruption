<script type="text/javascript">

function refeshCaptcha() {
	$("#captchaImg").attr("src", "/Captcha.jpg");
}
					 

function refreshPage() {
	window.location.replace("/ComplaintTracker");
}


// सबमिट करें 
function getFullComdetailofUser() {
	//debugger;
	var compCode = $('#compid').val();
	var capcha = $('#capta').val();
	var mob = $('#mobno').val();
	var randomkey = $('#randomkey').val();

	//alert(compCode+" "+capcha+"  "+randomkey+" "+mob);
	if ($.isNumeric(compCode)) {
		if (mob.length === 10 && $.isNumeric(mob)) {
			if (capcha.length === 4 && $.isNumeric(capcha)) {
					// send request
				$.ajax({
					type : 'GET',
					url : '/varifyCaptchawithRandomkeyAndMobileNo',
					async : false,
					data : {
						capthca : capcha,
						randomkey : randomkey,
						mobileno : mob,
						complaintcode : compCode
					},
					success : function(res) {
						
						console.log('res ==='+ res);
						
						if (res === 'OK') {
							openIframe(compCode, mob);

						} else {
							alert(res);
						}
					}

				});

				function openIframe(icompCode,imob) {

					//  alert(icompCode+" "+iemail+" "+imob);
					var h = ((window.innerHeight) - 110);
					var height = h + "px";

					$('#dvLoading').fadeIn();

					var link = "fullCompDetailsCorr?mob="
							+ imob + "&complanitcode="
							+ icompCode;

					var html = '<iframe src="' + link
							+ '"frameBorder=0 width="100%" height="'
							+ height
							+ '" allowfullscreen scrolling="yes"></iframe>';
					$('#forwordbody').html(html);
					$('#dvLoading').fadeOut();
					$('#modal-forword').modal('show');
					refeshCaptcha();
				}

			}else {
				alert("कृपया 4 अंकों का कैप्चा दर्ज करें");
			}

		} else {

			alert("कृपया 10 अंकों का मोबाइल नंबर दर्ज करें");
			return false;
		}

	} else {

		alert("कृपया सही शिकायत कोड दर्ज करें");
		return false;
	}
}


$(function () {
    $('#showbelow').css("display", "none");
});

</script>


