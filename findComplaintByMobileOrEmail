<script type="text/javascript">
function rejectOfficeLevel_work_tree(complaintscode,forwardid)
{ 	
	var h = ((window.top.innerHeight) - 110);
	var height = h + "px";
	$('#dvLoading').fadeIn();
	var compType=0;
	if(complaintscode.toString().substring(0,2)=='41')
		compType=41;
	else if(complaintscode.toString().substring(0,2)=='14') 
		compType=14;
	else
		compType=parseInt(complaintscode.toString().substring(0,1));
	var type = '';
    var link ='#';
 	link="ajax_getMarkingTree_app_status_work_tree?comp_code="+complaintscode+"&complaintType="+compType+"&forwardid="+forwardid;
	var html = '<iframe src="' + link + '"frameBorder=0 width="100%" height="'+height+'" allowfullscreen scrolling="yes"></iframe>';
	$('#modelbody').html(html);
	$('#modal-document').modal('show');
	$('#compNo').html("शिकायत संख्या - (" + complaintscode + ")");
	$('#dvLoading').fadeOut();
}	


</script>


<script type="text/javascript">
function reliefOtherAtr(complaintscode,actionid)
{ 	
	var h = ((window.top.innerHeight) - 110);
	var height = h + "px";
	$('#dvLoading').fadeIn();
	var compType=0;
	if(complaintscode.toString().substring(0,2)=='41')
		compType=41;
	else if(complaintscode.toString().substring(0,2)=='14') 
		compType=14;
	else
		compType=parseInt(complaintscode.toString().substring(0,1));
	var type = '';
    var link ='#';
    link="/ajaxCheck/DownloadATRAttachment?complaintId="+complaintscode+"&complaintType=14&actionid="+actionid+"&AtrOf1stOR2nd=TWO";
	var html = '<iframe src="' + link + '"frameBorder=0 width="100%" height="'+height+'" allowfullscreen scrolling="yes"></iframe>';
	$('#modelbody').html(html);
	$('#modal-document').modal('show');
	$('#compNo').html("अन्य दस्तावेज -शिकायत संख्या - (" + complaintscode + ")");
	$('#dvLoading').fadeOut();
}	
</script>

<script type="text/javascript">

function dscDocAtr(complaintscode, aid) {
	
	var h = ((window.top.innerHeight) - 110);
	var height = h + "px";
	
	$('#dvLoading').fadeIn();
	
	var compType=0;
	
	if(complaintscode.toString().substring(0,2)=='41')
		compType=41;
	else if(complaintscode.toString().substring(0,2)=='14') 
		compType=14;
	else
		compType=parseInt(complaintscode.toString().substring(0,1));
	var type = '';

	var type = '';
	var link = '#';
	link = "/ajaxCheck/DownloadAllAttachmentDsc?complaintId="
			+ complaintscode + "&complaintType=" + compType
			+ "&actionid=" + aid;
	var html = '<iframe src="' + link
			+ '"frameBorder=0 width="100%" height="' + height
			+ '" allowfullscreen scrolling="yes"></iframe>';
	$('#modelbody').html(html);
	$('#modal-document').modal('show');
	$('#compNo').html("शिकायत संख्या - (" + complaintscode + ")");
	$('#dvLoading').fadeOut();
}

</script>


