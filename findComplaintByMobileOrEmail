    <script>
    function sendOTP(){
      
        
     //   alert("sendOTP  ");
     // alert(hex_md5("sendOTP"));
        
//                    $("#sedOTPID").removeClass("btn-warning");
//                    $("#sedOTPID").addClass("btn-info");
//                    $('#sedOTPID').val("Re-Generate OTP/पुनः ओ०टी०पी० प्राप्त करें");
//                    $('#otp').css("display","block");
    var  captch  =  $('#capta').val();
    var  randomkey  =  $('#randomkey').val();
    var  mobile  =  $('#mobile_id').val();
    var  email  =  $('#email_id').val();
    
   // alert("-- "+captch+"  == "+randomkey+" >>"+mobile+" --"+email);
          
        // var compcode = document.getElementById("compid").value;
        if((mobile.length===10 && $.isNumeric(mobile)) || email!==""){
            
            
              if(email!==""){
                         
                         if(!isEmail(email)){
                             alert(" कृपया सही ईमेल आईडी दर्ज करें ");
                             return false;
                             
                         }
                     }
            
            
            $.ajax({
            type: "GET",
           // contentType: "application/json; charset=utf-8",
            url: "/senfOTPWithVrifyCapcha",
            data: {capthca:captch,randomkey:randomkey,mobile:mobile,email:email},
          
            success: function (data){
            
          //  alert(" --- "+data);
          
            
                if(data.length===6){
                    
                    alert("OTP आपके Mobile/Email पर भेज दिया गया है |");
                   $("#otpbutton").html('Re-Generate OTP/पुनः ओ०टी०पी० प्राप्त करें');
                    $("#otpbutton").removeClass("btn-warning");
                    $("#otpbutton").addClass("btn-info");
                   $('#varifyotpbutton').css("display","block");
                
                    
                    
                    
                    
                         
                }else{
                    alert(data); 
                }
               



            }
        });
        
    }else{
        alert("कृपया 10 अंकों का मोबाइल नंबर / ई-मेल आईडी दर्ज करें। ");
    }
    }
    </script>     
                                                           
   <script>
     function getVarifyOTPAndGetComplaintCode(){
          
       
         
        $("#otpbutton").html('Re-Generate OTP/पुनः ओ०टी०पी० प्राप्त करें');
       $("#otpbutton").removeClass("btn-warning");
       $("#otpbutton").addClass("btn-info");
//                    $("#sedOTPID").removeClass("btn-warning");
//                    $("#sedOTPID").addClass("btn-info");
//                    $('#sedOTPID').val("Re-Generate OTP/पुनः ओ०टी०पी० प्राप्त करें");
//                    $('#otp').css("display","block");
    var  captch  =  $('#capta').val();
    var  randomkey  =  $('#randomkey').val();
    var  mobile  =  $('#mobile_id').val();
    var  email  =  $('#email_id').val();
    var otp =  $('#txtforemail').val();
 //   alert("-- "+captch+"  == "+randomkey+" "+mobile+" "+email);
           $('#spin').css("display","block");
        // var compcode = document.getElementById("compid").value;
            $.ajax({
            type: "GET",
           // contentType: "application/json; charset=utf-8",
            url: "/getVarifyOTPAndGetComplaintCode",
            data: {capthca:captch,randomkey:randomkey,mobile:mobile,email:email,otp:otp},
          
            success: function (data){
            
           // alert(" --- "+data+"--"+JSON.stringify(data));
            console.log(" --- "+data+"--"+JSON.stringify(data));
             $('#spin').css("display","none");   
            
                if(data.sent_flag==='OK'){
                    refeshCaptcha();
                    option = '';
                    var  i=0;
                    
                    for(i=0;i<data.list1.length;i++){
                       option = option+'<tr><td>'+data.list1[i].complaintcode+'</td><td>'+data.list1[i].bfyname+'</td><td>'+data.list1[i].bfy_email+'</td><td>'+data.list1[i].bfy_mob+'</td><td>'+data.list1[i].appdateofreference+'</td></tr>'; 
                        
                    }
                     $('#tableid').css("display","block");
                     $('#title_text').css("display","block");
                    $('#tbodycomp').html(option);
                    
                         
                }else{
                    alert(data.sent_flag); 
                }
               



            }
        });    
       }
       </script>
                                                               
    
