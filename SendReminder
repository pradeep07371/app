    <script type="text/javascript">
        function _MarkOfficeLevel(title, src, width, height) {
            showmodalpopup(title, src, width, height);

        }
        function showmodalpopup(title, src, width, height) {

            $.colorbox({
                inline: true, title: "Print", iframe: false, href: "#popupdiv", open: true, width: '70%', height: '95%',
                onOpen: function () {
                    $('body').css('overflow', 'hidden');
                    $('#popupdiv').show();
                    $("#cboxTitle").css('display', 'none');
                },
                onClosed: function () {
                    $('body').css('overflow', 'auto');
                    $('#popupdiv').hide();

                }
            });



        }





    </script>
