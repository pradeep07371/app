    </script>  
    <script type="text/javascript">
        function validat_form() {

            var errMessage = "";

            var txtbxgvId = document.getElementById("compid").value;


            if (txtbxgvId === '') {

                errMessage += "कृपया शिकायत संख्या डाले!\n";
                return false;
            }
            if(!$.isNumeric(txtbxgvId)){
               errMessage="अमान्य शिकायत संख्या ";
               return false;   
                
            }
            else {

                $('#searchbtn').css("display", "none");
                $('#spin').css("display", "block");
                $.ajax({
                    type: "GET",
                    contentType: "application/json; charset=utf-8",
                    url: "/fetchReminderInfo",
                    data: {comp_id: txtbxgvId},
                    dataType: "json",
                    success: function (data) {

                        // console.log("data=== "+JSON.stringify(data)+"+"+data.errormsg);
                        if (data.errormsg === 'NOERROR') {
                            $('#showbelow').css("display", "block");
                            $('#spin').css("display", "none");
                            $('#mobno').val(data.bfy_mob1);
                            $('#email').val(data.bfy_email);

                        }
                        if (data.errormsg === 'NOCOMPLAINT') {

                            $('#showbelow').css("display", "none");
                            $('#spin').css("display", "none");
                            $('#searchbtn').css("display", "block");
                            alert("कोई शिकायत नहीं मिली,कृपया सही शिकायत संख्या दर्ज करें.");

                        }
                        if (data.errormsg === 'NODEFAULTERCOMPLAINT') {

                            $('#showbelow').css("display", "none");
                            $('#spin').css("display", "none");
                            $('#searchbtn').css("display", "block");
                            alert("your complaint is under process/आपकी शिकायत प्रक्रिया में है.");

                        }
                        if (data.errormsg === 'ERROR15') {

                            $('#showbelow').css("display", "none");
                            $('#spin').css("display", "none");
                            $('#searchbtn').css("display", "block");
                            alert("You can not send reminder before 15 day./आप 15 दिन से पहले अनुस्मारक नहीं भेज सकते हैं.");

                        }
                        if (data.errormsg === 'ERRORFAFD') {

                            $('#showbelow').css("display", "none");
                            $('#spin').css("display", "none");
                            $('#searchbtn').css("display", "block");
                            alert("संदर्भ निस्तारित हो चुका है इसलिए अनुस्मारक नहीं भेज सकते हैं.");

                        }
                        if (data.errormsg === 'NOCOMPLAINTEXISTS') {

                            $('#showbelow').css("display", "none");
                            $('#spin').css("display", "none");
                            $('#searchbtn').css("display", "block");
                            alert("No Complaint Exists./कोई शिकायत नहीं है.");


                        }



                    }
                });


            }



            if (errMessage !== "") {
                alert(errMessage);
                return false;
            }
        }


    </script>
    <script>
        //ओ० टी० पी० प्रविष्ट करें/Submit and Generate OTP
        function getOTP() {
            
            var captch = $('#capta').val();
            var randomkey = $('#randomkey').val();

            var compcode = document.getElementById("compid").value;
            $('#farefresh').html('<i class="fa fa-refresh fa-spin" style="font-size:24px"></i>');
            $.ajax({
                type: "GET",
                // contentType: "application/json; charset=utf-8",
                url: "/SendOPTreminder",
                data: {comp_id: compcode, captacha: captch, randomkey: randomkey},

                success: function (data) {


                    // alert("data=== response  "+data.length);
                      $('#farefresh').html('');
                    if (data.length === 4) {

                        alert("OTP आपके Mobile/Email पर भेज दिया गया है |");
                        $("#sedOTPID").removeClass("btn-warning");
                        $("#sedOTPID").addClass("btn-info");
                        $('#sedOTPID').val("Re-Generate OTP/पुनः ओ०टी०पी० प्राप्त करें");
                        $('#otp').css("display", "block");


                    }
                    if (data === 'NOCOMPLAINT') {
                        alert(" इस शिकायत संख्या पर कोई संदर्भ उपलब्ध नहीं है ");
                    }
                    if (data === 'ErrorNo15') {

                    }
                    if (data === 'You Entered invalidate Captcha Number.') {
                        alert("आपने गलत कैप्चा नंबर दर्ज किया है ।");
                    }
                    if (data === 'Regenrate OTP After 10 Second.') {
                        alert("10 सेकंड के बाद ओटीपी भेजें ।");
                    }



                }
            });
        }
    </script>
    <script>
        //सत्यापित ओ० टी० पी०/Verify OTP
        function showFullComplaint() {
            var compCode = $('#compid').val();
            var capcha = $('#capta').val();
            var otp = $('#otp_id').val();

            var randomkey = $('#randomkey').val();
            console.log(compCode+" "+capcha+"  "+randomkey+" "+otp);

            $.ajax({
                type: 'GET',
                url: 'varifyOTP',
                async: false,
                data: {capcha: capcha, otp: otp, SessRandomKey: randomkey},
                success: function (res) {
                  

                    if (res === 'OK') {
                        openIframe(compCode, capcha, otp);
                        refeshCaptcha();
                    } else {
                        alert(res);
                    }
                }

            });

            function openIframe(compCode, capcha, otp) {

                //  alert(icompCode+" "+iemail+" "+imob);
                var h = ((window.innerHeight) - 110);
                var height = h + "px";

                $('#dvLoading').fadeIn();

                var link = "fullComplaintDetail?complanitcode=" + compCode + "&otp=" + otp + "&randomkey=" + randomkey + "&captcha=" + capcha + "&comtype=0";

                var html = '<iframe src="' + link + '"frameBorder=0 width="100%" height="' + height + '" allowfullscreen scrolling="yes"></iframe>';
                $('#forwordbody').html(html);
                $('#dvLoading').fadeOut();
                $('#modal-forword').modal('show');

                //                $.ajax({
                //                      type:'GET',
                //                      url:'findCompTypebyCompCode',
                //                      data:{compcode:compCode,captch:capcha,otp:otp},
                //                     
                //                      success:function(res){
                //                          
                //                         
                //                          window.open("fullComplaintDetail?complanitcode="+compCode+"&otp="+otp+"&randomkey="+randomkey+"&captcha="+capcha+"&comtype=0", "_blank","titlebar=no,directories=no,location=no,toolbar=no,scrollbars=yes,resizable=yes,top=100,left=300,width=1800,height=700");
                // 
                //                      }
                //                 });
            }


        }
    </script>        

